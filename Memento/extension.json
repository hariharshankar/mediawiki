{
	"name": "Memento",
	"version": "2.1.5",
	"author": [
		"Harihar Shankar",
		"Herbert Van de Sompel",
		"Robert Sanderson",
		"Shawn M. Jones"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Memento",
	"descriptionmsg": "memento-desc",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"SpecialPages": {
		"TimeMap": "TimeMap",
		"TimeGate": "TimeGate"
	},
	"MessagesDirs": {
		"Memento": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"MementoAlias": "Memento.alias.php"
	},
	"AutoloadClasses": {
		"Memento": "Memento.php",
		"MementoResource": "MementoResource.php",
		"MementoResourceDirectlyAccessed": "MementoResourceDirectlyAccessed.php",
		"OriginalResourceDirectlyAccessed": "OriginalResourceDirectlyAccessed.php",
		"TimeMapResource": "TimeMapResource.php",
		"TimeMapFullResource": "TimeMapFullResource.php",
		"TimeMapPivotAscendingResource": "TimeMapPivotAscendingResource.php",
		"TimeMapPivotDescendingResource": "TimeMapPivotDescendingResource.php",
		"TimeMap": "TimeMap.php",
		"TimeGateResourceFrom302TimeNegotiation": "TimeGateResourceFrom302TimeNegotiation.php",
		"TimeNegotiator": "TimeNegotiator.php",
		"TimeGate": "TimeGate.php"
	},
	"Hooks": {
		"ArticleViewHeader": [ "Memento::onArticleViewHeader" ]
	},
	"config": {
		"MementoIncludeNamespaces": {
			"value": [
				0
			]
		},
		"MementoTimemapNumberOfMementos": {
			"value": 500
		},
		"MementoTimeNegotiationForThumbnails": {
			"value": false
		},
		"Memento": {
			"value": {}
		}
	},
	"manifest_version": 2
}
